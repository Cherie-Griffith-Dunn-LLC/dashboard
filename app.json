{
  "expo": {
    "name": "cyproteck",
    "description": "CYPROTECK Security Solutions Dashboard.",
    "privacy": "hidden",
    "slug": "cyproteck",
    "version": "1.0.0",
    "owner": "cyproteck",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "cyproteck",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyproteck.cyproteck",
      "buildNumber": "0.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cyproteck.cyproteck",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon.png",
      "name": "CYPROTECK",
      "description": "CYPROTECK Security Solutions Dashboard.",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components"
          ]
        }
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "cyproteck",
            "project": "dashboard"
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo",
      [
        "react-native-msal",
        {
          "androidPackageSignatureHash": "chGHORtE8XSnHOWZJryjlVX3zmU="
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8c5fc950-3942-4d7f-aa66-43e0f9e77cef"
      }
    }
  }
}
